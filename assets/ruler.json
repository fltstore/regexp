[
  {
    "id": 1,
    "title": "火车车次",
    "placeholder": "例如：G183,Z6,D98,K1214",
    "regular": "^[GCDZTSPKXLY1-9]\\d{1,4}$"
  },
  {
    "id": 2,
    "title": "手机机身码(IMEI)",
    "placeholder": "例如：97871849128314",
    "regular": "^\\d{15,17}$"
  },
  {
    "id": 3,
    "title": "手机号(mobile phone)中国(严谨), 根据工信部2019年最新公布的手机号段",
    "placeholder": "例如：13800000000",
    "regular": "^(?:(?:\\+|00)86)?1(?:(?:3[\\d])|(?:4[5-79])|(?:5[0-35-9])|(?:6[5-7])|(?:7[0-8])|(?:8[\\d])|(?:9[189]))\\d{8}$"
  },
  {
    "id": 1,
    "title": "必须带端口号的网址(或ip)",
    "placeholder": "例如：https://www.google.com:8081",
    "regular": "^((ht|f)tps?:\\/\\/)?[\\w-]+(\\.[\\w-]+)+:\\d{1,5}\\/?$"
  },
  {
    "id": 1,
    "title": "统一社会信用代码",
    "placeholder": "例如：91230184MABUFLT66",
    "regular": "^[0-9A-HJ-NPQRTUWXY]{2}\\d{6}[0-9A-HJ-NPQRTUWXY]{10}$"
  },
  {
    "id": 1,
    "title": "迅雷链接",
    "placeholder": "例如: thunder://eUEsICdtYWduZXQ6P3h0PXVybjpidGloOjBCQTE0RTUxRkUwNjU1RjE0Qzc4NjE4RjY4NDY0QjZFNTEyNjcyOUMnWlo=",
    "regular": "^thunderx?:\\/\\/[a-zA-Z\\d]+=$"
  },
  {
    "id": 1,
    "title": "ed2k链接(宽松匹配)",
    "placeholder": "例如: ed2k://|file|%E5%AF%84%E7%94%9F%E8%99%AB.PARASITE.2019.HD-1080p.X264.AAC-UUMp4(ED2000.COM).mp4|2501554832|C0B93E0879C6071CBED732C20CE577A3|h=5HTKZPQFYRKORN52I3M7GQ4QQCIHFIBV|/",
    "regular": "^ed2k:\\/\\/\\|file\\|.+\\|\\/$"
  },
  {
    "id": 1,
    "title": "子网掩码(不包含 0.0.0.0)",
    "placeholder": "例如: 255.255.255.0, 255.255.255.255, 255.240.0.0",
    "regular": "^(254|252|248|240|224|192|128)\\.0\\.0\\.0|255\\.(254|252|248|240|224|192|128|0)\\.0\\.0|255\\.255\\.(254|252|248|240|224|192|128|0)\\.0|255\\.255\\.255\\.(255|254|252|248|240|224|192|128|0)$"
  },
  {
    "id": 1,
    "title": "linux'隐藏文件'路径",
    "placeholder": "例如: /usr/ad/.dd, /root/.gitignore, /.gitignore",
    "regular": "^\\/(?:[^/]+\\/)*\\.[^/]*"
  },
  {
    "id": 1,
    "title": "linux文件夹路径",
    "placeholder": "例如: /usr/ad/dd/, /, /root/, / a a / a / a a /",
    "regular": "^\\/(?:[^/]+\\/)*$"
  },
  {
    "id": 1,
    "title": "linux文件路径",
    "placeholder": "例如: /root/b.ts, /root/abc",
    "regular": "^\\/(?:[^/]+\\/)*[^/]+$"
  },
  {
    "id": 1,
    "title": "window'文件夹'路径",
    "placeholder": "例如:C:\\Users\\Administrator\\Desktop\\qq",
    "regular": "^[a-zA-Z]:\\\\(?:\\w+\\\\?)*$"
  },
  {
    "id": 1,
    "title": "股票代码(A股)",
    "placeholder": "例如: sz000858, SZ002136, sz300675, SH600600, sh601155",
    "regular": "^(s[hz]|S[HZ])(000[\\d]{3}|002[\\d]{3}|300[\\d]{3}|600[\\d]{3}|60[\\d]{4})$"
  },
  {
    "id": 1,
    "title": "大于等于0, 小于等于150, 支持小数位出现5, 如145.5, 用于判断考卷分数",
    "placeholder": "例如: 150, 100.5",
    "regular": "^150$|^(?:\\d|[1-9]\\d|1[0-4]\\d)(?:\\.5)?$"
  },
  {
    "id": 1,
    "title": "html注释",
    "placeholder": "例如: <!--<div class='_bubble'></div>--><div>chenguzhen87</div><div class='_bubble'></div>-->",
    "regular": "<!--[\\s\\S]*?-->/g"
  },
  {
    "id": 1,
    "title": "md5格式(32位)",
    "placeholder": "例如: 21fe181c5bfc16306a6828c1f7b762e8",
    "regular": "^([a-f\\d]{32}|[A-F\\d]{32})$"
  },
  {
    "id": 1,
    "title": "GUID/UUID",
    "placeholder": "例如: e155518c-ca1b-443c-9be9-fe90fdab7345, 41E3DAF5-6E37-4BCC-9F8E-0D9521E2AA8D, 00000000-0000-0000-0000-000000000000",
    "regular": "^[a-f\\d]{4}(?:[a-f\\d]{4}-){4}[a-f\\d]{12}$"
  },
  {
    "id": 1,
    "title": "版本号(version)格式必须为X.Y.Z",
    "placeholder": "例如: 16.3.10",
    "regular": "^\\d+(?:\\.\\d+){2}$"
  },
  {
    "id": 1,
    "title": "视频(video)链接地址（视频格式可按需增删）",
    "placeholder": "例如: http://www.abc.com/video/wc.avi",
    "regular": "^https?:\\/\\/(.+\\/)+.+(\\.(swf|avi|flv|mpg|rm|mov|wav|asf|3gp|mkv|rmvb|mp4))$"
  },
  {
    "id": 1,
    "title": "base64格式",
    "placeholder": "例如: data:image/gif;base64,xxxx==",
    "regular": "^\\s*data:(?:[a-z]+\\/[a-z0-9-+.]+(?:;[a-z-]+=[a-z0-9-]+)?)?(?:;base64)?,([a-z0-9!$&',()*+;=\\-._~:@/?%\\s]*?)\\s*$"
  },
  {
    "id": 1,
    "title": "中文/汉字",
    "placeholder": "例如：你好",
    "regular": "^(?:[\\u3400-\\u4DB5\\u4E00-\\u9FEA\\uFA0E\\uFA0F\\uFA11\\uFA13\\uFA14\\uFA1F\\uFA21\\uFA23\\uFA24\\uFA27-\\uFA29]|[\\uD840-\\uD868\\uD86A-\\uD86C\\uD86F-\\uD872\\uD874-\\uD879][\\uDC00-\\uDFFF]|\\uD869[\\uDC00-\\uDED6\\uDF00-\\uDFFF]|\\uD86D[\\uDC00-\\uDF34\\uDF40-\\uDFFF]|\\uD86E[\\uDC00-\\uDC1D\\uDC20-\\uDFFF]|\\uD873[\\uDC00-\\uDEA1\\uDEB0-\\uDFFF]|\\uD87A[\\uDC00-\\uDFE0])+$"
  },
  {
    "id": 1,
    "title": "密码强度校验，最少6位，包括至少1个大写字母，1个小写字母，1个数字，1个特殊字符",
    "placeholder": "例如: WK@1212d12",
    "regular": "^\\S*(?=\\S{6,})(?=\\S*\\d)(?=\\S*[A-Z])(?=\\S*[a-z])(?=\\S*[!@#$%^&*? ])\\S*$"
  },
  {
    "id": 1,
    "title": "邮政编码(中国)",
    "placeholder": "例如: 734199",
    "regular": "^(0[1-7]|1[0-356]|2[0-7]|3[0-6]|4[0-7]|5[1-7]|6[1-7]|7[0-5]|8[013-6])\\d{4}$"
  },
  {
    "id": 1,
    "title": "email(支持中文邮箱)",
    "placeholder": "例如: 123@126.com",
    "regular": "^[A-Za-z0-9\\u4e00-\\u9fa5]+@[a-zA-Z0-9_-]+(\\.[a-zA-Z0-9_-]+)+$"
  }
]